{
	"name": "copy all tables",
	"properties": {
		"activities": [
			{
				"name": "copy all onprem tables",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderQuery": {
							"value": "SELECT sch.name AS SchemaName,\ntab.name AS TableName\nFROM sys.tables AS tab\nINNER JOIN\nsys.schemas AS sch\nON tab.schema_id =sch.schema_id\nWHERE sch.name= 'dbo'",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "SqlServeronprem",
						"type": "DatasetReference"
					}
				}
			}
		],
		"annotations": []
	}
}